{
  "name": "@jpl/explorer-1",
  "version": "1.0.0-alpha.3",
  "description": "Packaged styles and assets for DesignLab's design system, Explorer 1",
  "author": "DesignLab",
  "repository": {
    "type": "git",
    "url": "https://internal.ghe.server/18x/jpl-design-system"
  },
  "publishConfig": {
    "registry": "https://internal.npm.registry"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p watch browsersync",
    "build": "npm run build:clean && parcel build ./src/index.js --no-source-maps",
    "build:clean": "rimraf ./dist && rimraf .parcel-cache",
    "watch": "rimraf ./src/_app && parcel ./src/index.html --dist-dir ./src/.parcel/",
    "browsersync": "browser-sync start --config bs.config.js",
    "sync:base": "node ./utils/buildBase.js",
    "sync:components": "node ./utils/buildComponentScss.js",
    "sync:all": "node ./utils/buildBase.js && node ./utils/buildComponentScss.js"
  },
  "parcel-namer-rewrite": {
    "rules": {
      "index.js": "js/explorer-1.min.js",
      "(.*)\\.(.*)\\.(woff2|woff)": "fonts/$1.$3",
      "index.css": "css/explorer-1.min.css"
    }
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "autoprefixer": "^10.2.4",
    "browser-sync": "^2.26.14",
    "copyfiles": "^2.4.1",
    "glob": "^7.1.6",
    "lazysizes": "^5.3.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.2",
    "parcel-namer-rewrite": "^2.0.0-beta.1-8",
    "postcss": "^8.2.7",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "swiper": "^6.6.2"
  }
}

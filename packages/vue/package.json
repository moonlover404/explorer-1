{
  "name": "@explorer-1/vue",
  "version": "0.2.92",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "DesignLab",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa-jpl/explorer-1"
  },
  "type": "module",
  "scripts": {
    "dev": "pnpm prepare && vite",
    "build": "pnpm prepare && vue-tsc && vite build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "preview": "vite preview",
    "prepare": "pnpm prepare:fonts && pnpm prepare:public",
    "prepare:fonts": "cp -R ./node_modules/@explorer-1/common/src/fonts/ ./src/assets/fonts/",
    "prepare:public": "cp -R ./node_modules/@explorer-1/common/src/public/explorer-1/ ./public/explorer-1"
  },
  "prettier": "@explorer-1/prettier-config",
  "dependencies": {
    "@explorer-1/common": "workspace:*",
    "@fancyapps/ui": "^4.0.26",
    "@tailwindcss/forms": "^0.5.9",
    "animejs": "^3.2.2",
    "click-outside-vue3": "^4.0.1",
    "datebook": "^8.0.1",
    "dayjs": "^1.11.13",
    "fast-qs": "^2.0.3",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "sass": "^1.80.5",
    "swiper": "^11.1.3",
    "tailwindcss": "^3.4.14",
    "twitter-widgets": "^2.0.0",
    "vue": "^3.5.12",
    "vue-bind-once": "^0.2.1",
    "vue3-compare-image": "^1.2.5",
    "vue3-observe-visibility": "^1.0.2"
  },
  "devDependencies": {
    "@explorer-1/prettier-config": "workspace:*",
    "@vitejs/plugin-vue": "^5.1.4",
    "a11y-dialog": "^8.1.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "typescript": "^5.6.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.1.10"
  }
}
